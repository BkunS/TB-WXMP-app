/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.default-view {
  width: 100vw;
  margin: 0rpx 0;
  padding: 0rpx 0;
}

.default-image {
  width: 100%;
  top: 0;
  left: 0;
  right: 0;
  margin: 0rpx 0;
  padding: 0rpx 0;
  height: auto;
}

.default-font {
  font-family: noto_sans_sc_regular, FZYouHJW_ZhongXi, Helvetica, Tahoma, sans-serif!important;
}

.globalMsg-view {
  display: flex;
  position: fixed;
  margin: 0 auto;
  z-index: 9999;
  top: 0;
  left: 0; 
  right: 0;
}

.topNavBar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 20rpx;
  background-color: white;
}

.icon-view {
  display: flex;
  flex-direction: row;
  align-content: space-around;
}

.align-start {
  align-items: flex-start
}

.align-end {
  align-items: flex-end
}

.page-title-view {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  text-align: center;
  font-size: 32rpx;
}

.icon-image {
  height: 100%;
  padding: 0 20rpx;
}

.topNav-view {
  display: flex;
  flex-direction: column;
  z-index: 9999;
  margin: 0rpx 0;
  padding: 0rpx 0;
  position: fixed;
  top: 0;
  left: 0; 
  right: 0;
}
.topNav-image {
  z-index: 1;
  width: 100vw;
  left: 0; 
  right: 0;
}

.placeholder-view {
  display: flex;
  flex-direction: column;
  top: 0;
  left: 0;
  right: 0;
  padding: 0;
  margin: 0rpx 0;
}

.line {
  width:100%;
  height:2rpx;
  background: grey;
  margin: 20rpx 0;
}

.textLineThrough {
  text-decoration:line-through;
}

.list-wrapper-view {
  width: calc(100vw - 50rpx);
  padding: 0 25rpx;
  float: left;
}

.button-hover {
  color: grey;
  background-color: black;
}

@font-face {
  font-family: 'noto_sans_sc_regular';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
  url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
  url('notosanssc-regular-webfont.ttf') format('truetype');
   font-weight: normal;
   font-style: normal;
}